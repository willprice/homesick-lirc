begin
  prog = irexec
  button = KEY_VOLUMEUP
  config = pulsevolcontrol up
  repeat = 1
end

begin
  prog = irexec
  button = KEY_VOLUMEDOWN
  config = pulsevolcontrol down
  repeat = 1
end

begin 
  prog = irexec
  button = KEY_PLAYPAUSE
